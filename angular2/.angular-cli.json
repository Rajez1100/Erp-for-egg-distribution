{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular2"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "assets/css/material-dashboard.css?v=1.2.0",
        "assets/fonts/font-awesome-4.7.0/css/font-awesome.min.css",
        "assets/fonts/material-icons/material-icons.css",
        "assets/templates/kpi_dashboard/css/bootstrap.min.css",
        "assets/templates/kpi_dashboard/css/animate.css",
        "assets/templates/kpi_dashboard/css/style.css",
        "assets/templates/kpi_dashboard/css/plugins/toastr/toastr.min.css",
        "assets/templates/kpi_dashboard/js/plugins/gritter/jquery.gritter.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/tether/dist/js/tether.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "assets/templates/kpi_dashboard/js/plugins/metisMenu/jquery.metisMenu.js",
        "assets/templates/kpi_dashboard/js/plugins/slimscroll/jquery.slimscroll.min.js",
        "assets/templates/kpi_dashboard/js/plugins/pace/pace.min.js",
        "assets/templates/kpi_dashboard/js/plugins/flot/jquery.flot.js",
        "assets/templates/kpi_dashboard/js/plugins/flot/jquery.flot.tooltip.min.js",
        "assets/templates/kpi_dashboard/js/plugins/flot/jquery.flot.spline.js",
        "assets/templates/kpi_dashboard/js/plugins/flot/jquery.flot.resize.js",
        "assets/templates/kpi_dashboard/js/plugins/flot/jquery.flot.pie.js",
        "assets/templates/kpi_dashboard/js/plugins/peity/jquery.peity.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {},
    "serve": {
      "port": 4201
    }
  }
}
